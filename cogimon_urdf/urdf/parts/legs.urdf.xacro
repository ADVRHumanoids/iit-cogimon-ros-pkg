<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">

<xacro:if value="${COGIMON_LEFT_LEG}" >

<!-- LINKS -->
<link name ="LHipMot">
    <inertial>
      <origin xyz="${-4.1435899e+01*mmTom} ${-2.4099021e-01*mmTom} ${-6.7437774e+01*mmTom}" rpy="0 0 0"/>
      <mass value="2.0936467"/>
      <inertia ixx="${2.0328644e+04*mm2Tom2}"  ixy="${1.2836757e+01*mm2Tom2}"  ixz="${-7.1832791e+03*mm2Tom2}" iyy="${2.4404941e+04*mm2Tom2}" iyz="${2.2892687e+01*mm2Tom2}" izz="${8.9864540e+03*mm2Tom2}" />
    </inertial>
    <visual>
      <origin xyz="0. 0. 0." rpy="0 0 0."/>
      <material name="green"/>
      <geometry name="RHipMot_visual">
        <mesh filename="package://cogimon_urdf/meshes/HipRoll.stl" scale="${mmTom} ${mmTom} ${mmTom}" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.0 0. 0" rpy="0 0 0."/>
      <geometry name="RHipMot_collision">
        <mesh filename="package://cogimon_urdf/meshes/simple/HipRoll.stl" scale="${mmTom} ${mmTom} ${mmTom}" />
      </geometry>             
    </collision>
  </link>


<!-- JOINTS -->
<joint name="LHipLat" type="revolute">
    <parent link="Waist"/>
    <child link="LHipMot"/>
    <origin xyz="${15.45*mmTom} ${112.0*mmTom} ${-50.0*mmTom}" rpy="0 0 0" />
    <axis xyz="1 0 0" />
    <limit velocity="3.67" effort="176" lower="${-90.*toRad}" upper="${90.*toRad}" />
    <dynamics friction="0" damping="3.0"/>
</joint>

</xacro:if>


<xacro:if value="${COGIMON_RIGHT_LEG}" >

<!-- LINKS -->
<link name ="RHipMot">
    <inertial>
      <origin xyz="${-4.1435899e+01*mmTom} ${2.4099021e-01*mmTom} ${-6.7437774e+01*mmTom}" rpy="0 0 0"/>
      <mass value="2.0936467"/>
      <inertia ixx="${2.0328644e+04*mm2Tom2}"  ixy="${-1.2836757e+01*mm2Tom2}"  ixz="${-7.1832791e+03*mm2Tom2}" iyy="${2.4404941e+04*mm2Tom2}" iyz="${-2.2892687e+01*mm2Tom2}" izz="${8.9864540e+03*mm2Tom2}" />
    </inertial>
    <visual>
      <origin xyz="0.0 0 0" rpy="0 0 0"/>
      <material name="green"/>
      <geometry name="RHipMot_visual">
        <mesh filename="package://cogimon_urdf/meshes/HipRoll.stl" scale="${mmTom} ${-mmTom} ${mmTom}" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.0 0. 0" rpy="0 0 0."/>
      <geometry name="RHipMot_collision">
        <mesh filename="package://cogimon_urdf/meshes/simple/HipRoll.stl" scale="${mmTom} ${-mmTom} ${mmTom}" />
      </geometry>             
    </collision>
</link>

<!-- JOINTS -->
<joint name="RHipLat" type="revolute">
    <parent link="Waist"/>
    <child link="RHipMot"/>
    <origin xyz="${15.45*mmTom} ${-112.0*mmTom} ${-50.0*mmTom}" rpy="0 0 0" />
    <axis xyz="1 0 0" />
    <limit velocity="3.67" effort="176" lower="${-90.*toRad}" upper="${90.*toRad}" />
    <dynamics friction="0" damping="3.0"/>
</joint>

</xacro:if>



</robot>
